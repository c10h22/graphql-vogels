{
  "name": "graphql-vogels",
  "version": "0.0.1",
  "description": "Vogels helper for Graphql",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run check && npm run build",
    "check": "npm run lint && npm test",
    "lint": "eslint src",
    "build": "rm -rf lib\\* && babel src --ignore test --optional runtime --out-dir lib",
    "build-test": "npm run build && rm -rf test\\* && babel src/test --optional runtime --out-dir test",
    "test": "npm run build-test && mocha --require scripts/mocha-bootload test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c10h22/graphql-vogels.git"
  },
  "keywords": [
    "graphql",
    "vogels"
  ],
  "author": "Adnene KHALFA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c10h22/graphql-vogels/issues"
  },
  "homepage": "https://github.com/c10h22/graphql-vogels#readme",
  "devDependencies": {
    "aws-sdk": "~2.1.34",
    "babel": "^5.6.14",
    "babel-core": "^5.6.20",
    "babel-eslint": "^3.1.23",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "coveralls": "^2.11.2",
    "eslint": "^1.0.0-rc-1",
    "graphql": "^0.1.3",
    "istanbul": "^0.3.17",
    "joi": "^6.5.0",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "vogels": "^2.0.0-rc10"
  },
  "dependencies": {
    "babel-runtime": "^5.6.20",
    "lodash": "^3.10.0"
  }
}
